{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#b0b0eb",
    "activityBar.background": "#b0b0eb",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#d24d4d",
    "activityBarBadge.foreground": "#e7e7e7",
    "editorError.foreground": "#000000ff",
    "editorInfo.foreground": "#000000ff",
    "editorWarning.foreground": "#000000ff",
    "sash.hoverBorder": "#b0b0eb",
    "statusBar.background": "#8887e1",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#605ed7",
    "statusBarItem.remoteBackground": "#8887e1",
    "statusBarItem.remoteForeground": "#15202b",
    "commandCenter.border": "#15202b99",
    "titleBar.activeBackground": "#8887e1",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#8887e199",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#8887e1",
  "files.exclude": {
    ".vscode": true,
    ".trunk": true
  },
  "deno.enable": true,
  "deno.inlayHints.parameterTypes.enabled": true,
  "deno.inlayHints.variableTypes.enabled": true,
  "deno.suggest.completeFunctionCalls": true,
  "deno.unstable": true,
  "projectManager.groupList": true,
  "projectManager.openInNewWindowWhenClickingInStatusBar": true,
  "projectManager.supportSymlinksOnBaseFolders": true,
  "projectManager.git.maxDepthRecursion": 0,
  "projectManager.hg.maxDepthRecursion": 0,
  "[markdown]": {
    "editor.tabSize": 2,
    "editor.wrappingIndent": "same",
    "editor.cursorSurroundingLines": 0
  }
}